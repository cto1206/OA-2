<?xml version="1.0" encoding="UTF-8"?>

<jbpm-configuration>

  <process-engine-context>
    <command-service>
      <retry-interceptor />
      <environment-interceptor />
      <spring-transaction-interceptor current="true" />
    </command-service>
  </process-engine-context>

  <transaction-context>
	<hibernate-session current="true"/>
  </transaction-context>

</jbpm-configuration>